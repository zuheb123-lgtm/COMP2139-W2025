@model Project


@{
    ViewBag.Title = "Delete Project";
     
    Layout = "_Layout";
}


<h2>@ViewBag.Title</h2>


<h3>Are you sure you want to delete this project?</h3>
<div>
<h4>Project</h4>
<hr/>


<dl class="row">
    <dt class="col-sm-2">
        
        @Html.DisplayNameFor(model => model.Name)
        
    </dt>
    
    <dd class="col-sm-10">
        
        @Html.DisplayFor(model => model.Name,"ProjectName")
        
    </dd>
    
    <dt class="col-sm-2">
        
        @Html.DisplayNameFor(model => model.Description)
        
    </dt>
    
    <dd class="col-sm-10">
        
        @Html.DisplayFor(model => model.Description,"ProjectDescription")
        
    </dd>
    
    <dd class="col-sm-10">
        
        @Html.DisplayFor(model => model.ProjectId)
        
    </dd>
    
    <!-- Add other properties you wish to display -->
</dl>


<form asp-action="DeleteConfirmed" asp-route-id="@Model.ProjectId" method="post">

    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.ProjectId)
    <input type="submit" value="Delete" class="btn btn-danger"/>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    
</form>
</div>    